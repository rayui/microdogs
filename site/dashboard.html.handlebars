<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Notification Necklace</title>
    <link href="assets/css/bootstrap.css" rel="stylesheet">
    <link href="assets/css/general.css" rel="stylesheet">

    <script src="assets/js/jquery-1.7.1.js"></script>
    <script src="assets/js/underscore.js"></script>
    <script src="assets/js/backbone.js"></script>
    <script src="assets/js/mustache.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="assets/js/app.js"></script>
  </head>
  <body class="starter">
    <div class="container clearfix">
      <div class="header">
        <h2 class="text-muted">Notification Necklace</h2>
      </div>
      <div id="shock-list-info" class="jumbotron">
        <div id="fun-container">
          <div id="fun-left"><img src="assets/imgs/lighting-left.png" width="200" class="fun-shock" /></div>
          <div id="headshot" class="normal">
            <img src="assets/imgs/kris.png" width="200" height="268" class="fun-normal" />
            <img src="assets/imgs/kris-shock.png" width="200" height="268" class="fun-shock" />
          </div>
          <div id="fun-right"><img src="assets/imgs/lighting-right.png" width="200" class="fun-shock" /></div>
        </div>
        <div id="number-shocks" class="clearfix"><span id="actual-number-shocks">0</span></div>
        <div id="meter">
          <label>Vanity Meter: </label><meter id="actual-meter" value="0" min="0" max="100"></meter>
        </div>
        <div id="shock-list-container" class="clearfix">
          <ul id="shocker-list">
          </ul>
        </div>
      </div>
      <div id="shocker-leaderboard">
        <h4>Leaderboard</h4>
      </div>
    </div>
  </body>
</html>
